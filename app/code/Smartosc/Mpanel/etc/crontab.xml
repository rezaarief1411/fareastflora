<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="smartosc_group_sync_promotion">
        <job name="sync_promotion" instance="Smartosc\Mpanel\Cron\SyncPromotion"
             method="execute">
            <config_path>smartosc_mpanel_cron/cron_expr/sync_promotion</config_path>
        </job>
        <job name="sync_product_option" instance="Smartosc\Mpanel\Cron\SyncCustomOption"
             method="execute">
            <config_path>smartosc_mpanel_cron/cron_sync_product/sync_product_option</config_path>
        </job>
    </group>
</config>
