<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="disableFrontendBuilder" xsi:type="object">Smartosc\Mpanel\Console\DisableFrontendBuilder</item>
            </argument>
        </arguments>
    </type>
    <preference for="MGS\Mpanel\Block\Widget\OwlCarousel" type="Smartosc\Mpanel\Block\Widget\OwlCarousel"/>
    <type name="Magento\Catalog\Model\Config">
        <plugin name="sortProListByCreateDate" type="Smartosc\Mpanel\Plugin\ProductList\SortByCreateDate"/>
    </type>
    <preference for="MGS\Mmegamenu\Block\Mmegamenu" type="Smartosc\Mpanel\Block\Mmegamenu"/>
    <preference for="Magento\Theme\Block\Html\Breadcrumbs" type="Smartosc\Mpanel\Block\Html\Breadcrumbs"/>
    <preference for="Magento\Catalog\Block\Product\ProductList\Toolbar" type="Smartosc\Mpanel\Block\Product\ProductList\Toolbar"/>
    <type name="Magento\Catalog\Controller\Product\View">
        <plugin name="Smartosc_Mpanel_product_breadcrumbs" type="Smartosc\Mpanel\Plugin\Product\View" sortOrder="1"/>
    </type>
</config>
